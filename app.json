{
  "expo": {
    "name": "Event0",
    "slug": "evento",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#AFA98D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#AFA98D"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Pacifico.ttf",
            "./assets/fonts/Anton.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d945978f-0c0d-4c74-bee4-986eb5d05f60"
      }
    }
  }
}
